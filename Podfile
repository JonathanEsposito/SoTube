# Uncomment the next line to define a global platform for your project
platform :ios, '10.2'

target 'SoTube' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for SoTube
  pod 'Firebase/Core'
  pod 'Firebase/Auth'
  pod 'Firebase/Database'

  pod 'PayPal-iOS-SDK'

  target 'SoTubeTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'SoTubeUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
